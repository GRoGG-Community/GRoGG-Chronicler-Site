{
  "name": "stellarisrp-site",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run start:json\" \"npm run start:web\"",
    "build": "react-scripts build",
    "start:json": "npx json-server -p 3001 --routes routes.json db.json",
    "start:web": "react-scripts start --host 0.0.0.0"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "json-server": "^0.17.4",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
   "proxy": "http://localhost:3001"
}
